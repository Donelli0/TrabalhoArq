#include <DHT.h>

#define DHTPIN 7
#define DHTTYPE DHT11

DHT dht(DHTPIN, DHTTYPE);

void setup() {
  Serial.begin(9600);
  Serial.println("Teste: ");
  dht.begin();
  delay(2000);

}

void loop() {
  
  int h = dht.readHumidity();
  int t = dht.readTemperature();

  Serial.print("Humidade: ");
  Serial.print(h);
  Serial.print("%\t");
  Serial.print("Temperatura: ");
  Serial.print(t);
  Serial.println("Â°C");
  delay(2000);

}
